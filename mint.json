{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Chainbase",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#000000",
    "light": "#A5CFFF",
    "dark": "#000000",
    "background": {
      "light": "#FAFBFB",
      "dark": "#000000"
    }
  },
  "anchors": [
    {
      "icon": "code",
      "name": "Developer Console",
      "url": "https://console.chainbase.com/"
    },
    {
      "icon": "discord",
      "name": "Developer Discord",
      "url": "https://chainbase.com/discord"
    }
  ],
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@chainbase.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Start building",
    "url": "https://dashboard.mintlify.com"
  },
  "tabs": [
    {
      "name": "Data Catalog",
      "url": "data-catalog"
    },
    {
      "name": "Data Platform",
      "url": "data-platform"
    },
    {
      "name": "World Model",
      "url": "world-model"
    },
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "primaryTab": {
    "name": "Get Started"
  },
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintsearch"
  }
}
